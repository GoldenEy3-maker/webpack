{
  "config": {
    "dev": "--config __webpack__/dev.config.js",
    "prod": "--config __webpack__/prod.config.js"
  },
  "scripts": {
    "webpack-config-dev": "nodemon --watch \"./__webpack__/*\" --exec bun run start-dev",
    "webpack-config-prod": "nodemon --watch \"./__webpack__/*\" --exec bun run start-prod",
    "start-dev": "cross-env-shell webpack serve ${npm_package_config_dev}",
    "start-prod": "cross-env-shell webpack serve ${npm_package_config_prod}",
    "build-dev": "cross-env-shell webpack ${npm_package_config_dev}",
    "build-prod": "cross-env-shell webpack ${npm_package_config_prod} --stats-children",
    "clean": "rd /s /q build",
    "lint": "eslint src --ext .js --ext .ts",
    "lint-fix": "eslint src --ext .js --ext .ts --fix"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.4.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@types/node": "^22.13.13",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "babel-loader": "^10.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "eslint": "^9.23.0",
    "eslint-plugin-react": "^7.37.4",
    "favicons-webpack-plugin": "^6.0.1",
    "filemanager-webpack-plugin": "^8.0.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "image-minimizer-webpack-plugin": "^4.1.3",
    "imagemin": "^9.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^8.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^11.0.1",
    "mini-css-extract-plugin": "^2.9.2",
    "nodemon": "^3.1.9",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.1.5",
    "sass-loader": "^16.0.5",
    "typescript": "^5.8.2",
    "typescript-plugin-css-modules": "^5.1.0",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  }
}
